{
  "useTabs": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 120,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": [
    "./node_modules/prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteSortOrder": "options-scripts-markup-styles",
        "svelteBracketNewLine": false
      }
    }
  ]
}
